{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": ["./lib", "index.js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "template": "node_modules/clean-jsdoc-theme",
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "theme_opts": {
        "create_style": "h1.page-title::after, article h1::after{background: #e6e6e6}"
      }
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "search": true
  }
}